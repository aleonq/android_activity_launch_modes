<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="notes.apps.aleonqe.com.activitylaunchflags">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="false"
        android:theme="@style/AppTheme">

        <!--Stanrard Activities-->
        <activity
            android:name=".standard.Standard_1"
            android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".standard.Standard_2"
            android:launchMode="standard" />
        <activity
            android:name=".standard.Standard_3"
            android:launchMode="standard" />
        <activity
            android:name=".standard.Standard_4"
            android:launchMode="standard" />

        <!--Single Instance Activities-->
        <activity
            android:name=".single_instance.SingleInstance_2"
            android:launchMode="singleInstance" />
        <activity
            android:name=".single_instance.SingleInstance_3"
            android:launchMode="singleInstance" />
        <activity
            android:name=".single_instance.SingleInstance_4"
            android:launchMode="singleInstance" />
        <activity
            android:name=".single_instance.SingleInstance_1"
            android:launchMode="singleInstance" />

        <!--Single Top Activities-->
        <activity
            android:name=".single_top.SingleTop_1"
            android:launchMode="singleTop" />
        <activity
            android:name=".single_top.SingleTop_2"
            android:launchMode="singleTop" />
        <activity
            android:name=".single_top.SingleTop_3"
            android:launchMode="singleTop" />
        <activity
            android:name=".single_top.SingleTop_4"
            android:launchMode="singleTop" />

        <!--Single Task Activities-->
        <activity
            android:name=".single_task.SingleTask_1"
            android:launchMode="singleTask" />
        <activity
            android:name=".single_task.SingleTask_2"
            android:launchMode="singleTask" />
        <activity
            android:name=".single_task.SingleTask_3"
            android:launchMode="singleTask" />
        <activity
            android:name=".single_task.SingleTask_4"
            android:launchMode="singleTask" />

        <service android:name=".ActivityLauncherService" />
    </application>

</manifest>